{
    "extends": ["tslint:recommended", "tslint-config-prettier"],
    "rules": {
        "no-console": false,
        "no-duplicate-variable": true,
        "no-unused-variable": [
            true
        ],
        "interface-name": false,
        "object-literal-sort-keys": false,
        "no-var-requires": false,
        "trailing-comma": true
    }
}